    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha",
    "test-on-travis": "istanbul cover --report lcovonly ./node_modules/mocha/bin/_mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strikeentco/yarl.git"
  },
  "bugs": {
