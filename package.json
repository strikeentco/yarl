  },
  "dependencies": {
    "multi-part": "^2.0.0"
  },
  "devDependencies": {
    "body-parser": "^1.16.1",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "express": "^4.14.1",
    "mocha": "^3.2.0",
    "multer": "^1.3.0",
    "should": "^11.2.0"
